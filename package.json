{
  "name": "go-test-highlighter",
  "displayName": "go-test-highlighter",
  "description": "Highlights Go test files",
  "version": "0.0.7",
  "publisher": "makarychev13",
  "engines": {
    "vscode": "^1.91.0"
  },
  "categories": [
    "Other"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/makarychev13/go-test-highlighter"
  },
"activationEvents": [
  "*"
],
"main": "./out/extension.js",
"contributes": {
  "themes": [],
  "colors": [
    {
      "id": "goTestFileDecoration.background",
      "defaults": {
        "dark": "#006400",
        "light": "#90EE90",
        "highContrast": "#00FF00"
      },
      "description": "Background color for Go test files."
    }
  ]
}, 
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.91.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  }
}
